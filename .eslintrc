{
  "root": true,
  "extends": "@antfu",
  "rules": {
    "vue/singleline-html-element-content-newline": "off",
    "vue/multiline-html-element-content-newline": "off",
    "vue/html-self-closing":["error", {
        "html": {
          "void": "always",
          "normal": "always",
          "component": "always"
        },
        "svg": "always",
        "math": "always"
      }
    ]
  },
  "plugins": ["plugin:vue/vue3-strongly-recommended"]
}